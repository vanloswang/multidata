[cube]
s0_mipmap = false
s0_skydome = true
s0_skydome_image = /usr/share/backgrounds/Murales_by_Jan_Bencini.jpg

[core]
s0_active_plugins = core;composite;opengl;decor;resize;mousepoll;gnomecompat;place;compiztoolbox;move;grid;firepaint;session;shift;shelf;annotate;water;regex;imgpng;vpswitch;imgjpeg;thumbnail;group;wobbly;animation;wallpaper;workarounds;cube;td;animationaddon;mag;showmouse;rotate;scale;unitymtgrabhandles;cubeaddon;expo;ezoom;switcher;staticswitcher;ring;unityshell;
s0_hsize = 6
s0_vsize = 1

[ring]
s0_next_key = <Shift><Control><Primary><Super>Tab
s0_prev_key = Disabled
s0_next_all_key = Disabled
s0_prev_all_key = Disabled

[switcher]
s0_next_key = <Alt><Super>Tab
s0_prev_key = Disabled
s0_next_all_key = Disabled
s0_prev_all_key = Disabled

[shift]
s0_initiate_key = <Shift><Super>q
s0_next_key = Disabled
s0_prev_key = Disabled
s0_prev_all_key = <Control><Primary><Super>Tab
s0_mode = 1

[mag]
s0_initiate = <Super>n

[cubeaddon]
s0_deformation = 0
s0_top_images = /usr/share/backgrounds/London_Eye_From_Beneath_by_Fernando_García.jpg;
s0_bottom_images = /usr/share/backgrounds/Floorboards_by_Dawid_Huczyński.jpg;

[animation]
s0_focus_effects = animation:None;
s0_focus_durations = 500;
s0_all_random = true
s0_dodge_mode = 0
s0_horizontal_folds_num_folds = 4
s0_wave_amp_mult = 1.500000

[expo]
s0_deform = 2
s0_reflection = true

[wallpaper]
s0_bg_image = /usr/share/backgrounds/Golden_Bloom_by_Twinmama.jpg;/usr/share/backgrounds/Flocking_by_noombox.jpg;/usr/share/backgrounds/Morning_Dew_by_Lars_Clausen.jpg;/usr/share/backgrounds/Precise_Pangolin_by_Vlad_Gerasimov.jpg;/usr/share/backgrounds/The_Forbidden_City_by_Daniel_Mathis.jpg;/usr/share/backgrounds/Twilight_Frost_by_Phil_Jackson.jpg;
s0_bg_image_pos = 0;0;0;0;0;0;
s0_bg_fill_type = 0;0;0;0;0;0;
s0_bg_color1 = #000000ff;#000000ff;#000000ff;#000000ff;#000000ff;#000000ff;
s0_bg_color2 = #000000ff;#000000ff;#000000ff;#000000ff;#000000ff;#000000ff;

[staticswitcher]
s0_next_all_key = Disabled
s0_prev_all_key = Disabled

[group]
s0_select_single_key = <Super>z
s0_tabmode_key = <Super>b

[ezoom]
s0_zoom_in_button = <Super>Button4
s0_zoom_out_button = <Super>Button5

